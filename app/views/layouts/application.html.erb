<!DOCTYPE html>
<html>
<head>
  <title>Welcome to Pinterview</title>
<!--   <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
 -->  
<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
<%= csrf_meta_tags %>

</head>

<body>
  <% if current_user %>
    <div class="create"> 
      <a class="my-button"><%= link_to "Home", root_path, html_options = {:class => "my-button"} %>
      <%= link_to "New Pin", new_pin_path, html_options = {:class => "my-button"} %>
      <%= link_to "My Profile", current_user, html_options = {:class => "my-button"} %>
      <%= link_to "Log out", logout_path, method: "delete", class: "my-button" %>
    </div>
  <% end %>
  <%= yield %>
</body>
</html>

